<Window x:Class="PseudoGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="LoadMagFilterButton" Content="Load Mag-Filter" HorizontalAlignment="Left" Margin="90,113.53,0,0" VerticalAlignment="Top" Width="140"
                Command="{Binding LoadMagFilterCommand}" IsEnabled="False"/>
        <Button x:Name="UnloadMagFilterButton" Content="Unload Mag-Filter" HorizontalAlignment="Left" Margin="90,152,0,0" VerticalAlignment="Top" Width="140"
                Command="{Binding ShutdownMagFilterCommand}" IsEnabled="False"/>
        <Button x:Name="StartupMagFilterButton" Content="Startup Mag-Filter" HorizontalAlignment="Left" Margin="264,114,0,0" VerticalAlignment="Top" Width="140"
        	Command="{Binding LoadMagFilterCommand}"/>
        <Button x:Name="ShutdownMagFilterButton" Content="Shutdown Mag-Filter" HorizontalAlignment="Left" Margin="264,152,0,0" VerticalAlignment="Top" Width="140"
        	Command="{Binding ShutdownMagFilterCommand}"/>

    </Grid>
</Window>
