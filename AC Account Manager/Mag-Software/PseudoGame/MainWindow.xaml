<Window x:Class="PseudoGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="LoadMagFilterButton" Content="Load Mag-Filter" HorizontalAlignment="Left" Margin="90,113.53,0,0" VerticalAlignment="Top" Width="140"
                Command="{Binding LoadMagFilterCommand}"/>
        <Button Content="Shutdown Mag-Filter" HorizontalAlignment="Left" Margin="90,152,0,0" VerticalAlignment="Top" Width="140"
                Command="{Binding ShutdownMagFilterCommand}"/>

    </Grid>
</Window>
