<Window x:Class="ThwargLauncher.DiagnosticsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DiagnosticsWindow" Height="300" Width="300">
    <Grid>
        <TextBox Text="{Binding Path=DiagnosticInfo,Mode=OneWay}" HorizontalAlignment="Stretch" Margin="0,0,0,0" TextWrapping="Wrap" VerticalAlignment="Stretch" />
    </Grid>
</Window>
